cd $RHINO_STAGE || exit
ls -l 
for FILE in `ls -1 *.mp3` ; do
    grep -e $FILE $RHINO_LOG && continue
    while true; do    
        echo -n $FILE : 
        afplay -t 30 $FILE 
        say -r 777 beep
        read a
        [ "$a" == "" ] && continue
        echo $FILE $a >> $RHINO_LOG
        break
    done
done
